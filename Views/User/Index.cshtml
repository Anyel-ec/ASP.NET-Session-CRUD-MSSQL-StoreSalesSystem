@model List<TiendaCRUD.Models.TableViewModel.UserTableViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class=" row">
     <div class=col-6">
         <table class="table table-bordered">
               <tr>
                   <th>Nombre</th>
                   
                   <th>Correo</th>
                   
                </tr>
             @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Nombre</td>
                        <td>@item.Correo</td>
                    </tr>
                }


         </table>
     </div>

</div>

